        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Contact.Booster_verified__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Created by: MH
Business Unit: HR
Purpose: To confirm employees&apos;s COVID booster verification</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
