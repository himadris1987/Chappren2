            <field>Contact.Partially_Vaccinated__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Created by:YB,MH
Business Unit: HR
Purposes :- To follow up employees vaccine status.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>HR_Partially_Vaccinated_Time_based_email_alert</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Contact.Estimated_Second_Dose_Date__c</offsetFromField>
            <timeLength>9</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>HR - Vaccine Verification</fullName>
