    <rules>
        <fullName>HR - Booster  Status  Notification and Follow-up</fullName>
        <actions>
            <name>HR_Employees_Booster_Status_notification</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Contact.Got_Booster__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Created by:YB,MH
Business Unit: HR
Purposes :- To follow up employees vaccine status.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>HR - Booster Eligibility</fullName>
        <actions>
            <name>Booster_Eligibility_Reminde_for_Employee</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Booster_Eligibility_Reminde_for_HR</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Created by: YB,MH
Bussines unit:- HR
Purpose :-  To Remind employees and HR to upload their Booster card.</description>
        <formula>Booster_Eligibility_Date__c =  TODAY()</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>HR - Booster Verification</fullName>
        <actions>
            <name>HR_Employees_Booster_Submission_Confirmation</name>
