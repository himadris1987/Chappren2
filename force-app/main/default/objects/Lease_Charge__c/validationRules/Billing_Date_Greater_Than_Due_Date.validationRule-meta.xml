<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_Date_Greater_Than_Due_Date</fullName>
    <active>true</active>
    <errorConditionFormula>Billing_Date__c &gt;  DATEVALUE(Due_Date__c)</errorConditionFormula>
    <errorMessage>The Billing Date cannot be greater than the Due Date.</errorMessage>
</ValidationRule>
